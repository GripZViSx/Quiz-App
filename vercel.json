{
   "version": 2,
   "builds": [
      {
         "src": "server.js",
         "use": "@now/node-server"
    }
  ],
   "routes": [
      { "src": "/api/data", "dest": "/server.js" },
      { "src": "/home", "dest": "/server.js" },
      { "src": "/about", "dest": "/server.js" },
      { "src": "/contacts", "dest": "/server.js" },
      { "src": "/(.*)", "dest": "/server.js" }
  ]
}