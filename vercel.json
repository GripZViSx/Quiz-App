{
   "version": 2,
   "builds": [
      { "src": "index.js", "use": "@now/node" },
      { "src": "api/welcome.js", "use": "@now/node" },
      { "src": "api/questions.js", "use": "@now/node" },
      { "src": "api/results.js", "use": "@now/node" }
   ],
   "routes": [
      { "src": "/api/welcome", "dest": "api/welcome.js" },
      { "src": "/api/questions", "dest": "api/questions.js" },
      { "src": "/api/results", "dest": "api/results.js" },
      { "src": "/(.*)", "dest": "/index.js" }
   ]
}